select * 
from `data_marts.transaction_products`
where transaction_id = "119137611243696";


select * 
from `data_marts.quality_issues`
where magento_order = "119137611243696";


select * 
from `data_lake.refunds`
where magento_order = "119137611243696";

select * 
from `data_marts.transaction_products`
where email_address in (select email_address from `data_marts.transactions` where transaction_id = "119137611243696")
and transaction_date > "2024-11-19";
